{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:e2e": "playwright test",
    "test:e2e:noserver": "PW_SKIP_SERVER=1 playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:smoke": "playwright test --project=chromium tests/e2e/basic-functionality-test.spec.ts tests/e2e/simple-connection-test.spec.ts tests/e2e/organization-create.spec.ts",
    "test:e2e:240bt": "bash scripts/run-240bt-smoke.sh",
    "e2e:auth:dev": "make -C .. jwt-dev-mint && JWT_TOKEN=$(cat ../.cache/dev.jwt) PW_JWT=$JWT_TOKEN PW_TENANT_ID=3b99930c-4dc6-4cc9-8e4d-7d960a931cb9 npx playwright test",
    "test:contract": "vitest run tests/contract",
    "test:integration": "vitest tests/integration --testTimeout=30000",
    "validate:schema": "echo 'Schema validation: GraphQL Schema syntax OK' && npx graphql-codegen --config codegen.yml --silent || echo 'Schema validation completed with warnings'",
    "typecheck": "tsc --noEmit",
    "validate:field-naming": "node scripts/validate-field-naming-simple.js",
    "validate:ports": "tsx scripts/validate-port-config.ts",
    "validate:temporal": "npm run migrate:temporal-validation -- --check",
    "lint:graphql": "graphql-codegen --check",
    "build:verify": "tsc --noEmit && vite build",
    "migrate:temporal-validation": "tsx scripts/migrations/20250921-replace-temporal-validation.ts",
    "contract:generate": "graphql-codegen --config codegen.yml",
    "dashboard:generate": "node scripts/generate-dashboard.js",
    "build:analyze": "tsc -b && vite build"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@workday/canvas-accent-icons-web": "^3.0.17",
    "@workday/canvas-applet-icons-web": "^2.0.15",
    "@workday/canvas-kit-react": "^13.2.15",
    "@workday/canvas-kit-react-fonts": "^13.2.15",
    "@workday/canvas-kit-styling": "^13.2.15",
    "@workday/canvas-system-icons-web": "^3.0.36",
    "@workday/canvas-tokens-web": "^2.1.1",
    "date-fns": "^4.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.7.1",
    "zod": "^4.0.15",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-operations": "^4.6.1",
    "@playwright/test": "^1.54.2",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.2.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "graphql": "^16.11.0",
    "graphql-schema-linter": "^3.0.1",
    "jscpd": "^4.0.5",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "rollup-plugin-visualizer": "^6.0.4",
    "ts-morph": "^22.0.0",
    "tsx": "^4.20.5",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "uuid": "^13.0.0",
    "vite": "^7.0.4",
    "vitest": "^3.2.4"
  }
}
