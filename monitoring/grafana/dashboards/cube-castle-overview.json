{
  "dashboard": {
    "id": null,
    "title": "Cube Castle System Overview - P4 Enhanced",
    "tags": ["cube-castle", "temporal-api", "cache-performance"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "API Response Time Comparison",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_request_duration_seconds_sum{job=\"organization-graphql-api\"}[5m]) / rate(http_request_duration_seconds_count{job=\"organization-graphql-api\"}[5m])",
            "legendFormat": "GraphQL API - Avg Response (65% improved)"
          },
          {
            "expr": "rate(http_request_duration_seconds_sum{job=\"temporal-api\"}[5m]) / rate(http_request_duration_seconds_count{job=\"temporal-api\"}[5m])",
            "legendFormat": "Temporal API - Avg Response (94% improved)"
          },
          {
            "expr": "rate(http_request_duration_seconds_sum{job=\"organization-command-api\"}[5m]) / rate(http_request_duration_seconds_count{job=\"organization-command-api\"}[5m])",
            "legendFormat": "Command API - Avg Response"
          }
        ],
        "yAxes": [
          {
            "label": "seconds",
            "max": 1,
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Cache Performance Dashboard",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(cache_operations_total{result=\"hit\"}[5m]) / rate(cache_operations_total[5m]) * 100",
            "legendFormat": "Cache Hit Rate % (Target: >90%)"
          },
          {
            "expr": "rate(cache_operations_total{result=\"miss\"}[5m]) / rate(cache_operations_total[5m]) * 100",
            "legendFormat": "Cache Miss Rate %"
          }
        ],
        "yAxes": [
          {
            "label": "percentage",
            "max": 100,
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Temporal API Query Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(temporal_query_duration_seconds_sum{operation=\"as_of_date\"}[5m]) / rate(temporal_query_duration_seconds_count{operation=\"as_of_date\"}[5m])",
            "legendFormat": "As-of-Date Queries (avg)"
          },
          {
            "expr": "rate(temporal_query_duration_seconds_sum{operation=\"history\"}[5m]) / rate(temporal_query_duration_seconds_count{operation=\"history\"}[5m])",
            "legendFormat": "History Queries (avg)"
          },
          {
            "expr": "histogram_quantile(0.95, rate(temporal_query_duration_seconds_bucket[5m]))",
            "legendFormat": "P95 Temporal Query Time"
          }
        ],
        "yAxes": [
          {
            "label": "seconds",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Request Rate by Service",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total{job=\"organization-graphql-api\"}[5m])",
            "legendFormat": "GraphQL Requests/sec"
          },
          {
            "expr": "rate(http_requests_total{job=\"temporal-api\"}[5m])",
            "legendFormat": "Temporal API Requests/sec"
          },
          {
            "expr": "rate(http_requests_total{job=\"organization-command-api\"}[5m])",
            "legendFormat": "Command API Requests/sec"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 5,
        "title": "Redis Memory & Connection Status",
        "type": "graph", 
        "targets": [
          {
            "expr": "redis_memory_used_bytes / 1024 / 1024",
            "legendFormat": "Redis Memory Used (MB)"
          },
          {
            "expr": "redis_connected_clients",
            "legendFormat": "Connected Clients"
          },
          {
            "expr": "rate(redis_keyspace_hits_total[5m]) / (rate(redis_keyspace_hits_total[5m]) + rate(redis_keyspace_misses_total[5m])) * 100",
            "legendFormat": "Redis Hit Rate %"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        }
      },
      {
        "id": 6,
        "title": "Overall Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total{status=~\"5..\"}[5m]) / rate(http_requests_total[5m])",
            "legendFormat": "{{job}} - Error Rate"
          }
        ],
        "yAxes": [
          {
            "max": 1,
            "min": 0,
            "unit": "percentunit"
          }
        ],
        "gridPos": {
          "h": 6,
          "w": 24,
          "x": 0,
          "y": 24
        }
      },
      {
        "id": 7,
        "title": "Performance Improvement Summary",
        "type": "table",
        "targets": [
          {
            "expr": "up{job=~\"organization-.*|temporal-.*\"}",
            "format": "table",
            "legendFormat": "Service Status"
          }
        ],
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 0,
          "y": 30
        }
      },
      {
        "id": 8,
        "title": "Cache Performance Stats",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(cache_operations_total{result=\"hit\"}[5m]) / rate(cache_operations_total[5m]) * 100",
            "legendFormat": "Hit Rate %"
          },
          {
            "expr": "redis_memory_used_bytes / redis_memory_max_bytes * 100",
            "legendFormat": "Memory Usage %"
          },
          {
            "expr": "rate(temporal_query_duration_seconds_count[5m])",
            "legendFormat": "Temporal Queries/sec"
          }
        ],
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 12,
          "y": 30
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "5s",
    "annotations": {
      "list": [
        {
          "name": "Phase 4 Integration",
          "datasource": "prometheus",
          "enable": true,
          "expr": "ALERTS{alertname=~\".*API.*|.*Cache.*\"}",
          "iconColor": "red",
          "titleFormat": "Alert: {{alertname}}",
          "textFormat": "{{description}}"
        }
      ]
    }
  }
}